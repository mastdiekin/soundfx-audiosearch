{
   "default_locale": "ru",
   "background": {
      "scripts": [ "./js/jquery.min.js", "./js/locale.js", "./js/audiosearch.js" ],
      "persistent": true
   },
   "description": "Аудиопоиск по сайтам со звуками",
   "icons": {
      "128": "./img/icon-large.png",
      "16": "./img/icon-bitty.png",
      "48": "./img/icon-small.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "42",
   "name": "Sound FX Search",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "browser_action": {
      "default_icon": "./img/icon-small.png",
      "default_popup": "popup.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "permissions": [ "https://translate.yandex.net/api/v1.5/tr/translate", "contextMenus", "storage", "notifications", "tabs" ],
   "version": "1.0.0"
}
